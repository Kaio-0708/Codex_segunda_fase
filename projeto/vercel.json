{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/criar-conta", "dest": "/server.js" },
    { "src": "/login", "dest": "/server.js" },
    { "src": "/editar/(.*)", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
