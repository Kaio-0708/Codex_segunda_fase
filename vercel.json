{
  "version": 2,
  "builds": [
    {
      "src": "projeto/public/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "projeto/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "projeto/server.js"
    },
    {
      "src": "/api/criar-conta",
      "dest": "projeto/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "projeto/public/$1"
    }
  ]
}
